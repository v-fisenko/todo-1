<%= link_to project_path(project), remote: true do %>
  <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span>
<% end %> &nbsp;

<%= link_to 'javascript:void(0)', id: "edit_project_#{project.id}" do %>
  <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
<% end %> &nbsp;

<%= link_to 'javascript:void(0)', class: "handle" do %>
  <span class="glyphicon glyphicon-move" aria-hidden="true"></span>
<% end %> &nbsp;

<%= link_to project_path(project), method: :delete, remote: true, data: { confirm: "Are you sure?" }, id: "delete_project_#{project.id}" do%>
  <span class="glyphicon glyphicon-trash"></span>
<% end %>
